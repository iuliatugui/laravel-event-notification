{
    "name": "ivfuture/laravel-event-notification",
    "description": "This package provides an easy way to integrate notification with Laravel 6, Redis and socket.io .",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Iulia Tugui",
            "email": "iulia.tugui@ivfuture.uk"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Ivfuture\\EventNotification\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "scripts": {
        "pre-package-install": [
            "./script.sh"
        ],
        "post-package-install": [
            "./script.sh"
        ],
        "pre-install-cmd": [
            "./script.sh"
        ],
        "post-install-cmd": [
            "./script.sh"
        ],
        "pre-autoload-dump": [
            "./script.sh"
        ],
        "post-autoload-dump": [
            "./script.sh"
        ],
        "pre-dependencies-solving": [
            "./script.sh"
        ],
        "post-dependencies-solving": [
            "npm install express socket.io ioredis redis  --save"
        ]


    }
}
